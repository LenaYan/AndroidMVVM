<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.ray.mvvm.sample"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".app.SampleApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.main.MainActivity"
            android:label="@string/title_activity_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.simple.SimplePageActivity"
            android:label="@string/title_activity_simple_page" />
        <activity
            android:name=".view.simple.SimpleDataPageActivity"
            android:label="@string/title_activity_simple_data_page" />
        <activity
            android:name=".view.event.EventBusActivity"
            android:label="@string/title_activity_event_bus" />
        <activity
            android:name=".view.event.EventBusNextActivity"
            android:label="@string/title_activity_event_bus_next" />
        <activity
            android:name=".view.list.SimpleListActivity"
            android:label="@string/title_activity_simple_list" />
        <activity
            android:name=".view.list.TestListActivity"
            android:label="@string/title_activity_test_list" />
    </application>

</manifest>